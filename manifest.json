{
  "name": "UI Color Palette",
  "id": "1063959496693642315",
  "api": "1.0.0",
  "editorType": [
    "figma",
    "figjam"
  ],
  "relaunchButtons": [
    { "command": "edit", "name": "Edit the UI Color Palette" }
  ],
  "permissions": [
    "payments",
    "currentuser"
  ],
  "networkAccess": {
    "allowedDomains": [
      "https://figma.com",
      "https://rsms.me",
      "https://noteforms.com",
      "https://corsproxy.io"
    ]
  },
  "parameters": [
    {
      "name": "Preset",
      "key": "preset",
      "description": "Select a color system"
    },
    {
      "name": "Color space",
      "key": "space",
      "description": "Select a specific color space"
    },
    {
      "name": "Palette layout",
      "key": "view",
      "description": "Select a specific layout"
    },
    {
      "name": "Palette name",
      "key": "name",
      "allowFreeform": true,
      "optional": true
    }
  ],
  "parameterOnly": false,
  "main": "dist/code.js",
  "ui": "dist/ui.html"
}
